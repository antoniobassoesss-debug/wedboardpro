{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/client",
  "framework": null,
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
